<h2>Edit a task list</h2>
<!--, url: id_path-->
<%= form_for @task_list, html: { class: "signin-form" } do |f| %>

  <% f.object.errors.full_messages.each do |error| %>
    <div class="errors">
      <%= error %>
    </div>
  <% end %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-field" %>
  </div>
  <div>
    <%= f.submit "Update Task List", class: "submit-button", disabled: "disabled" %>
  </div>

<% end %>